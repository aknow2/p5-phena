{
  "name": "p5-phena",
  "displayName": "phena",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:p5-phena.run"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "p5-phena.run",
        "title": "Run p5 phena"
      }
    ],
    "keybindings": [
      {
        "command": "p5-phena.run",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus && resourceFilename =~ /\\.p5\\.js$/"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.36.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
